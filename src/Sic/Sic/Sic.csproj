<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>8</LangVersion>
        <Authors>Serhii Buta</Authors>
        <Description>Single Item Cache</Description>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/Otaman/Sic</PackageProjectUrl>
        <Version>1.0.0</Version>
        <PackageIcon>icon.png</PackageIcon>
        <Copyright></Copyright>
        <RepositoryUrl>https://github.com/Otaman/Sic</RepositoryUrl>
        <PackageTags>Cache;AsyncCache;BackgroundUpdate</PackageTags>
    </PropertyGroup>

    <ItemGroup>
        <None Include="..\..\..\icon.png">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
    </ItemGroup>

</Project>
